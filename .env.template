# Claude Code Development Environment
# Copy this to .env and fill in your values

# Sentry - Error Tracking & Monitoring
SENTRY_DSN=
SENTRY_ENVIRONMENT=development
SENTRY_TRACES_SAMPLE_RATE=0.1
SENTRY_PROFILES_SAMPLE_RATE=0.1

# Cloudflare - Deployment & Infrastructure
CLOUDFLARE_API_TOKEN=
CLOUDFLARE_ACCOUNT_ID=
CLOUDFLARE_ZONE_ID=

# Linear - Project Management (if not using MCP config)
LINEAR_API_KEY=

# PR Agent (Qodo) - Code Review
QODO_API_KEY=
GITHUB_TOKEN=

# Development
NODE_ENV=development
DEBUG=false

# Protomaps API Key (get yours at https://protomaps.com/api)
VITE_PROTOMAPS_API_KEY=your_api_key_here
